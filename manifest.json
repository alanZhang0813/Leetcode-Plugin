{
    "manifest_version": 3,
    "name": "LeetCode Profile Viewer",
    "version": "1.0",
    "description": "View your LeetCode profile data.",
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "host_permissions": [
      "https://leetcode.com/graphql/*"
    ]
  }
  