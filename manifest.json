{
    "manifest_version": 3,
    "name": "LeetCode Profile Viewer",
    "version": "1.0",
    "description": "View your LeetCode profile data.",
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://leetcode.com/graphql/*"
    ]
  }
  